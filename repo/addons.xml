<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.nineau-catchup" name="NineAU catch-up" provider-name="grb" version="1.1">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension library="addon.py" point="xbmc.python.pluginsource" provides="video">
    <provides>video</provides>
  </extension>
  <extension library="addon.py" point="xbmc.addon.metadata">
    <summary>Nine Australia catch-up</summary>
    <description>Channel Nine (Australia) catch-up TV.</description>
    <provides>video</provides>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.grb.xbmc" name="grb's XBMC Addon Repository" provider-name="grb" version="1.01">
  <extension point="xbmc.addon.repository" name="grb's XBMC Addon Repository">
    <info compressed="false">https://raw.github.com/grantbeattie/grb.xbmc.repository/master/repo/addons.xml</info>
    <checksum>https://raw.github.com/grantbeattie/grb.xbmc.repository/master/repo/addons.xml.md5</checksum>
    <datadir zip="true">https://raw.githubusercontent.com/grantbeattie/grb.xbmc.repository/master/repo/</datadir>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Install grb's Addons</summary>
    <description>Audio and video addons by grb</description>
    <platform>all</platform>
  </extension>
</addon>
</addons>
